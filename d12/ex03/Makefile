NAME = ft_hexdump
SRCS = ft_hexdump.c ft_hexdump_print.c

all: build

build:
	gcc -Wall -Werror -Wextra -o $(NAME) $(SRCS)

clean:

fclean: clean
	rm -f $(NAME)
